  "scripts": {
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "dev": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "dev:express": "nodemon server.js",
    "dev:storybook": "concurrently \"yarn dev\" \"yarn storybook\"",
    "lint": "eslint --ext js,ts,tsx src",
    "start:express": "node server.js",
    "start:nginx": "nginx -c ~/Projects/brxm-multi-site-project/frontend/react/nginx.conf",
    "stop:nginx": "nginx -s stop",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@bloomreach/eslint-config-react": "1.0.2",
