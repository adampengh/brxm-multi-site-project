{
  "name": "@bloomreach/example-react",
  "description": "Example React SPA for Bloomreach Experience",
  "version": "14.6.1",
  "private": true,
  "dependencies": {
    "@bloomreach/connector-components-react": "^14.7.2",
    "@bloomreach/react-sdk": "^14.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "axios": "^0.21",
    "express-http-proxy": "^1.6.3",
    "http-proxy-middleware": "^2.0.1",
    "node-sass": "^4.0.0",
    "query-string": "^7.1.0",
    "react": "^16.14",
    "react-currency-format": "^1.0.0",
    "react-dom": "^16.14",
    "react-gtm-module": "^2.0.11",
    "react-router-dom": "^5.2",
    "react-slick": "^0.28.1",
    "slick-carousel": "^1.8.1",
    "typescript": "4.0.5",
    "universal-cookie": "^4.0.4"
  },
  "scripts": {
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "dev": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "lint": "eslint --ext js,ts,tsx src",
    "start-express": "node server.js",
    "start-nginx": "nginx -c ~/Projects/brxm-multi-site-project/frontend/react/nginx.conf",
    "stop-nginx": "nginx -s stop",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/node-logger": "^6.4.9",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.9",
    "@types/react": "^16.9",
    "@types/react-currency-format": "^1.0.0",
    "@types/react-dom": "^16.9",
    "@types/react-gtm-module": "^2.0.1",
    "@types/react-router-dom": "^5.1",
    "@types/react-slick": "^0.23.8",
    "eslint": "^7.12",
    "eslint-config-react-app": "^6.0",
    "react-scripts": "4.0.0"
  }
}
